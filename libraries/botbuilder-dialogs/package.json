  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "@microsoft/recognizers-text-choice": "1.1.4",
    "@microsoft/recognizers-text-date-time": "1.1.4",
    "@microsoft/recognizers-text-number": "1.1.4",
    "@microsoft/recognizers-text-suite": "1.1.4",
    "@types/node": "^10.12.18",
