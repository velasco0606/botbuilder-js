    "cldr-data": "^35.1.0",
    "globalize": "^1.4.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.47",
    "codelyzer": "^4.1.0",
    "mocha": "^5.2.0",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0"
  },
  "scripts": {
    "test": "tsc && nyc mocha tests/",
    "build": "tsc",
